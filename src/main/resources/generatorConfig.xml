<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<!--suppress MybatisGenerateCustomPluginInspection -->
<generatorConfiguration>
    <context id="Mysql" targetRuntime="MyBatis3Simple" defaultModelType="flat">
        <property name="javaFileEncoding" value="UTF-8"/>
        <property name="useMapperCommentGenerator" value="false"/>

        <plugin type="tk.mybatis.mapper.generator.MapperPlugin">
            <property name="mappers" value="tk.mybatis.mapper.common.Mapper"/>
            <property name="caseSensitive" value="true"/>
            <property name="forceAnnotation" value="true"/>
            <property name="beginningDelimiter" value="`"/>
            <property name="endingDelimiter" value="`"/>
        </plugin>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://docker.windnet.cn:3306/teamsking"
                        userId="teamsking"
                        password="123456">
        </jdbcConnection>

        <javaModelGenerator targetPackage="com.teamsking.domain.entity"
                            targetProject="src/main/java/"/>

        <sqlMapGenerator targetPackage="mapper"
                         targetProject="src/main/resources"/>

        <javaClientGenerator targetPackage="com.teamsking.domain.repository"
                             targetProject="src/main/java"
                             type="XMLMAPPER"/>

        <table tableName="recommend_cooperation">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="recommend_open">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="recommend_teacher">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="school">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="school_about">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="school_carousel">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="school_friendship">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="school_manager">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="school_news">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="study_note">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="study_questionnaire">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="study_score">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="study_visit">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="study_vote">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="study_wrong_question">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="sys_config">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="tag">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="user_category">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="user_tag">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>

        <table tableName="course_evaluate">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>
    </context>
</generatorConfiguration>
